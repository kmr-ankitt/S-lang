let a = 0;
let temp;

// Fibonacci Sequence using roll loop (For loop alternative)
roll (let b = 1; a < 10000; b = temp + b) {
  echo a;
  temp = a;
  a = b;
}

// Recusive message loop(While loop alternative)
let message = "nice shit";
let i = 1;

loop(i <= 5) {
    echo message;
    i = i + 1;
}


